rep1='s'
while rep1=='s':
        nome=input('Digite o nome do funcionario: ')
        salario=float(input('Digite o sal치rio do funcionario: '))
        print('Haver치 um reajuste de 10% no salario do funcionario cadastrado.')
        reajuste=salario*0.10
        sa_novo=salario+reajuste
        print('Nome do funcionario: ',nome,'\nSal치rio: ',salario,'\nReajuste: ',reajuste,'\nSal치rio Novo: ',sa_novo)
        rep1=input('Deseja fazer outro cadastrado? (s/n)')
